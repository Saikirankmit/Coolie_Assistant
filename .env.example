# Copy this file to .env and fill in the real values before running the server.
# Keep the .env file secret and do NOT commit it to source control.

# Supabase (server-side) - required
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# n8n webhook endpoints (used by the server to dispatch reminders)
N8N_WHATSAPP_WEBHOOK=https://n8n.example.com/webhook/whatsapp-trigger
N8N_GMAIL_WEBHOOK=https://n8n.example.com/webhook/gmail-trigger

# Optional: generic proxy fallback
N8N_WEBHOOK_URL=https://n8n.example.com/webhook

# Dev server port (optional)
PORT=5051
